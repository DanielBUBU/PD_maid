{
  "name": "play-dl",
  "version": "1.9.4",
  "description": "YouTube, SoundCloud, Spotify, Deezer searching and streaming for discord-js bots",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "tsup",
    "build:check": "tsc --noEmit --incremental false",
    "pretty": "prettier --config .prettierrc \"play-dl/*.ts\" \"play-dl/*/*.ts\" \"play-dl/*/*/*.ts\" --write ",
    "prepublishOnly": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/play-dl/play-dl"
  },
  "keywords": [
    "node-youtube-dl",
    "youtube-dl",
    "yt-dl",
    "ytdl",
    "youtube",
    "spotify-dl",
    "spotify",
    "soundcloud",
    "deezer"
  ],
  "author": "Killer069",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/play-dl/play-dl/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/play-dl/play-dl#readme",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/node": "^17.0.12",
    "prettier": "^2.3.1",
    "tsup": "^5.11.11",
    "typedoc": "^0.22.11",
    "typedoc-plugin-extras": "^2.2.1",
    "typedoc-plugin-missing-exports": "^0.22.4",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "play-audio": "^0.5.2"
  }
}
